<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>tienda.servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login2</servlet-name>
        <servlet-class>tienda.servlet.login2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cliente</servlet-name>
        <servlet-class>tienda.servlet.cliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Presupuesto</servlet-name>
        <servlet-class>tienda.servlet.Presupuesto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>prueba2</servlet-name>
        <servlet-class>tienda.servlet.prueba2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login2</servlet-name>
        <url-pattern>/login2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cliente</servlet-name>
        <url-pattern>/cliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Presupuesto</servlet-name>
        <url-pattern>/Presupuesto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>prueba2</servlet-name>
        <url-pattern>/prueba2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
